{
    "name": "huangdijia/laravel-redis-ide-helper",
    "description": "redis-ide-helper for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "redis",
        "ide-helper"
    ],
    "homepage": "https://github.com/huangdijia/laravel-redis-ide-helper",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Huangdijia\\RedisIdeHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\RedisIdeHelper\\RedisIdeHelperServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src"
    }
}